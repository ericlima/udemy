<h1>show article<h1>

<%= form_for @article do |f| %>
  <p>
    <%= f.label :title %><br/>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %><br/>
    <%= f.text_area :description %>
  </p>
  
<% end %>
  
<%= link_to "edit",edit_article_path(@article) %> | <%= link_to "articles", articles_path %>